    "release": "npm run clean && npm run build"
  },
  "dependencies": {
    "@game.object/ts-game-toolbox": "2.2.2",
    "p5": "^1.1.9",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^1.2.1"
  },
  "devDependencies": {
    "@types/p5": "^0.9.1",
    "@types/parcel-env": "^0.0.0",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/semantic-ui": "^2.2.7",
    "parcel": "^1.12.4",
    "typescript": "^4.0.2"
  }
}
